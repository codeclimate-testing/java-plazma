	<queryString><![CDATA[SELECT 
 F.ORGANIZATION_NAME,
 F.DOCUMENT_NO,
 F.DOCUMENT_DATE,
 D_WRH.NAME AS OUTCOME_WAREHOUSE_NAME,
 C_WRH.NAME AS INCOME_WAREHOUSE_NAME,
 F.CURRENCY_NAME,
 F.CURRENCY_RATE,
 F.CURRENCY_RATE_DATE,
 P.PRODUCT_NAME,
 U.NAME AS UNIT_NAME,
 I.QUANTITY,
 I.CURRENCY_PRICE_WITH_TAX AS CURRENCY_ITEM_PRICE,
 I.CURRENCY_AMOUNT_WITH_TAX AS CURRENCY_ITEM_AMOUNT,
 F.CURRENCY_DOCUMENT_AMOUNT,
 I.PRICE_WITH_TAX AS ITEM_PRICE,
 I.AMOUNT_WITH_TAX AS ITEM_AMOUNT,
 F.DOCUMENT_AMOUNT
 FROM INVENTORY_MOVE D
 INNER JOIN V_FINANCE_DOCUMENT F ON F.ID = D.ID
 INNER JOIN INVENTORY_MOVE_ITEM I ON I.INVENTORY_MOVE_ID = D.ID
 INNER JOIN V_PRODUCT P ON P.ID = I.PRODUCT_ID
 INNER JOIN UNIT U ON U.ID = I.UNIT_ID
 LEFT JOIN V_WAREHOUSE D_WRH ON D_WRH.ID = D.OUTCOME_WAREHOUSE_ID
 LEFT JOIN V_WAREHOUSE C_WRH ON C_WRH.ID = D.INCOME_WAREHOUSE_ID
 /*LEFT JOIN V_RESPONSIBLE D_RE ON D_RE.ID = D.OUTCOME_RESPONSIBLE_ID
 LEFT JOIN V_RESPONSIBLE C_RE ON C_RE.ID = D.INCOME_RESPONSIBLE_ID*/
 WHERE D.ID = $P{ID}]]></queryString>

