<ui xmlns="http://plazmaforge.org/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form name="DocumentStatusListForm" type="ListForm" title="@{DocumentStatusListForm.title}">
		<content layout="FitLayout">
			<children>
				<table>
					<columns>
						<column text="@{DocumentStatusListForm.documentColumn.text}" property="documentType" data-type="String" width="300"/>
						<column text="@{DocumentStatusListForm.codeColumn.text}" property="code" data-type="String" width="200"/>
						<column text="@{DocumentStatusListForm.nameColumn.text}" property="name" data-type="String" width="200"/>
					</columns>
				</table>
			</children>
		</content>
	</form>
</ui>