	<queryString><![CDATA[SELECT
 AL.ORGANIZATION_ID,
 AL.PERIOD_ID,
 AL.LEDGER_ACCOUNT_ID,
 A.CODE AS LEDGER_ACCOUNT_CODE,
 A.NAME AS LEDGER_ACCOUNT_NAME,
 R.DEBIT_AMOUNT AS REST_DEBIT_AMOUNT,
 R.CREDIT_AMOUNT AS REST_CREDIT_AMOUNT,
 C.CORR_LEDGER_ACCOUNT_ID,
 CA.CODE AS CORR_LEDGER_ACCOUNT_CODE,
 CA.NAME AS CORR_LEDGER_ACCOUNT_NAME,
 C.OPERATION_TYPE,
 C.AMOUNT,
 C.BALANCE_AMOUNT
FROM V_RPT_LDG_ACCOUNT_ALL AL
INNER JOIN LEDGER_ACCOUNT A ON A.ID = AL.LEDGER_ACCOUNT_ID
LEFT JOIN V_RPT_LDG_ACCOUNT_REST_START R
  ON (R.LEDGER_ACCOUNT_ID = AL.LEDGER_ACCOUNT_ID AND R.ORGANIZATION_ID = $P{ORGANIZATION_ID} AND R.PERIOD_ID = $P{PERIOD_ID})
LEFT JOIN V_RPT_LDG_ACCOUNT_CORR C
  ON (C.LEDGER_ACCOUNT_ID = AL.LEDGER_ACCOUNT_ID  AND C.ORGANIZATION_ID = $P{ORGANIZATION_ID} AND C.PERIOD_ID = $P{PERIOD_ID})
LEFT JOIN LEDGER_ACCOUNT CA ON CA.ID = C.LEDGER_ACCOUNT_ID
WHERE AL.ORGANIZATION_ID = $P{ORGANIZATION_ID} AND AL.PERIOD_ID = $P{PERIOD_ID}
ORDER BY AL.ORGANIZATION_ID, AL.PERIOD_ID, AL.LEDGER_ACCOUNT_ID]]></queryString>

