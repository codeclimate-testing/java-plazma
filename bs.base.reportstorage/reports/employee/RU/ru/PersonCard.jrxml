	<queryString><![CDATA[SELECT
 E.ID,
 E.CODE,
 C.FIRST_NAME,
 C.MIDDLE_NAME,
 C.LAST_NAME,
 C.GENDER,
 C.BIRTH_DATE,
 PC.DOCUMENT_SERIES,
 PC.DOCUMENT_NO,
 PC.DOCUMENT_DATE,
 PC.DOCUMENT_GIVEN,
 PC.DIPLOMA_TYPE,
 PC.DIPLOMA_SERIES,
 PC.DIPLOMA_NO,
 PC.DIPLOMA_DATE,
 PC.DIPLOMA_QUALIFICATION,
 PC.IDN,
 PD.NAME AS DOCUMENT_TYPE,
 FS.NAME AS FAMILY_STATUS,
 NC.NAME AS NATIONALITY_COUNTRY,
 PC.LANGUAGE_ID,
 SP.NAME AS DIPLOMA_SPECIALITY,
 PC.COUNTRY_RESIDENCE_ID,
 E.PERSON_ID,
 E.TOTAL_SENIORITY_YEAR,
 E.TOTAL_SENIORITY_MONTH,
 E.TOTAL_SENIORITY_DAY,
 E.UNBROKEN_SENIORITY_YEAR,
 E.UNBROKEN_SENIORITY_MONTH,
 E.UNBROKEN_SENIORITY_DAY,
 A.FULL_ADDRESS_STRING AS ADDRESS_STRING,
 H.PHONE_STRING

FROM PERSON_CARD PC
 INNER JOIN PERSON P ON (PC.PERSON_ID = P.ID)
 INNER JOIN CONTACT C ON (C.ID = P.ID)
 INNER JOIN EMPLOYEE E ON (P.ID = E.PERSON_ID)
 LEFT JOIN COUNTRY NC ON (NC.ID = PC.NATIONALITY_ID)
 LEFT JOIN SPECIALITY SP ON (SP.ID =  PC.DIPLOMA_SPECIALITY_ID)
 LEFT JOIN PERSON_DOCUMENT_TYPE PD ON (PD.ID =  PC.DOCUMENT_TYPE_ID)
 LEFT JOIN FAMILY_STATUS FS ON (FS.ID = PC.FAMILY_STATUS_ID)
 LEFT JOIN V_DEF_CONTACTABLE_ADDRESS A ON A.OWNER_ID = P.ID
 LEFT JOIN V_DEF_CONTACTABLE_PHONE H ON H.OWNER_ID = P.ID
WHERE E.ID = $P{ID}]]></queryString>

