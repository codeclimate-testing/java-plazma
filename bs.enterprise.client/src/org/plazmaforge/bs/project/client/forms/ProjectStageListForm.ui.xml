<ui xmlns="http://plazmaforge.org/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form name="ProjectStageListForm" type="ListForm" title="@{ProjectStageListForm.title}">
		<content layout="FitLayout">
			<children>
				<table>
					<columns>
						<column text="@{ProjectStageListForm.nameColumn.text}" property="name" data-type="String" width="200"/>
						<column text="@{ProjectStageListForm.orderNumberColumn.text}" property="orderNumber" data-type="Integer" width="100"/>
						<column text="@{ProjectStageListForm.fromBeginColumn.text}" property="fromBegin" data-type="Integer" width="100"/>
						<column text="@{ProjectStageListForm.averangeDurationColumn.text}" property="averangeDuration" data-type="Integer" width="100"/>
					</columns>
				</table>
			</children>
		</content>
	</form>
</ui>