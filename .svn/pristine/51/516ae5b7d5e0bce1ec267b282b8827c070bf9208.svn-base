	<queryString><![CDATA[SELECT
 E.ID AS EMPLOYEE_ID,
 E.CODE AS EMPLOYEE_CODE,
 C.FIRST_NAME,
 C.LAST_NAME,
 C.MIDDLE_NAME,
 T.WORK_DAY,
 T.WORK_HOUR,
 T.PAYROLL_AMOUNT,
 T.START_BALANCE,
 T.END_BALANCE,
 T.PAYROLL_AMOUNT,
 R.PAYROLL_DAY,
 R.PAYROLL_HOUR,
 R.AMOUNT,
 Y.CODE AS PAYROLL_CODE,
 Y.NAME AS PAYROLL_NAME,
 Y.PAYROLL_TYPE_ID
FROM EMPLOYEE E
INNER JOIN CONTACT C ON C.ID = E.PERSON_ID
INNER JOIN EMPLOYEE_TOTAL T ON T.EMPLOYEE_ID = E.ID AND T.PERIOD_ID = $P{PERIOD_ID}
INNER JOIN EMPLOYEE_PAYROLL R ON R.EMPLOYEE_ID = E.ID AND R.PERIOD_ID = $P{PERIOD_ID}
INNER JOIN PAYROLL Y ON Y.ID = R.PAYROLL_ID
WHERE E.ORGANIZATION_ID = $P{ORGANIZATION_ID}
ORDER BY E.CODE, E.ID, Y.PAYROLL_TYPE_ID]]></queryString>

