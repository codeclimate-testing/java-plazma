	</parameter>
	<queryString><![CDATA[SELECT
 P.ID,
 P.PRODUCT_CODE,
 P.PRODUCT_NAME,
 U.NAME AS UNIT_NAME,
 SUM(I.QUANTITY * I.UNIT_RATE) AS QUANTITY,
 SUM(I.AMOUNT) AS AMOUNT
FROM PURCHASE_PLAN D
INNER JOIN V_FINANCE_DOCUMENT F ON F.ID = D.ID
INNER JOIN PURCHASE_PLAN_ITEM I ON I.PURCHASE_PLAN_ID = D.ID
INNER JOIN V_PRODUCT P ON P.ID = I.PRODUCT_ID
INNER JOIN UNIT U ON P.UNIT_ID = U.ID
 WHERE F.ORGANIZATION_ID =  $P{ORGANIZATION_ID}
  AND F.DOCUMENT_DATE >=  $P{PERIOD_START_DATE}
  AND F.DOCUMENT_DATE <=  $P{PERIOD_END_DATE}
GROUP BY P.ID, P.PRODUCT_CODE, U.NAME, P.PRODUCT_NAME
ORDER BY P.PRODUCT_NAME, P.ID]]></queryString>

