<entity-mapping>
    <entity class="org.plazmaforge.bsolution.carservice.common.beans.CarServOrderInventory" table="CAR_SERV_ORDER_INVENTORY">
    	<attributes>
        	<key name="id" column="ID" type="Integer" generator-type="increment"/> 
        	<reference name="damage" column="CAR_SERV_ORDER_DAMAGE_ID" class="org.plazmaforge.bsolution.carservice.common.beans.CarServOrderDamage"  required="true" />
        	<reference name="carPaintType" column="CAR_PAINT_TYPE_ID" class="org.plazmaforge.bsolution.carservice.common.beans.CarPaintType"/>
        	<attribute name="inventoryDate" column="INVENTORY_DATE" type="Date"/>
        </attributes>
    </entity>
</entity-mapping>
