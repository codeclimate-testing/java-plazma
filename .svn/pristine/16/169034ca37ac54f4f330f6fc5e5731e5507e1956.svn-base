
    <entity xmlns="http://plazmaforge.org/erm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="PartnerMove" class="org.plazmaforge.bsolution.finance.common.beans.PartnerMove" table="PARTNER_MOVE">
    	<attributes>
        	<key name="id" column="ID" type="Integer" generator-type="increment"/>
        	<reference name="owner" column="OWNER_ID" class="org.plazmaforge.bsolution.finance.common.beans.FinanceDocument" required="true"/>        
        	<attribute name="partnerId" column="PARTNER_ID" type="Integer" required="true"/>
        	<attribute name="amount" column="AMOUNT" type="Double"/>
        	<attribute name="amountWithTax" column="AMOUNT_WITH_TAX" type="Double"/>
        	<attribute name="taxAmount" column="TAX_AMOUNT" type="Double"/>
        </attributes>        
    </entity>

