
    <entity xmlns="http://plazmaforge.org/erm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="ProjectStageLink" class="org.plazmaforge.bs.project.shared.entities.ProjectStageLink" table="PROJECT_STAGE_LINK">
