	<queryString><![CDATA[SELECT
 P1.ID AS PROJECT_ID,
 1 AS RESULT_MODE,
 P1.NAME AS PROJECT_NAME,
 PP1.YEAR_NUMBER,
 PP1.MONTH_NUMBER,
 PP1.MONTH_NAME,
 PP1.DAY_NUMBER,
 PP1.DAY_POS
FROM V_PROJECT_PERIOD_DAY PP1
LEFT JOIN V_PROJECT_DATE_EXT PD1 ON (PD1.ORGANIZATION_ID = $P{ORGANIZATION_ID} AND PP1.DAY_POS >= PD1.START_DAY_POS AND PP1.DAY_POS <= PD1.END_DAY_POS)
LEFT JOIN PROJECT P1 ON (P1.ID = PD1.ID)
WHERE PP1.DAY_POS >= $P{START_DAY_POS} 
 AND PP1.DAY_POS <= $P{END_DAY_POS}]]></queryString>

