	<parameter name="ORGANIZATION_ID" isForPrompting="false" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="PERIOD_START_DATE" isForPrompting="false" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date(1900,1,1)]]></defaultValueExpression>
	</parameter>
	<parameter name="PERIOD_END_DATE" isForPrompting="false" class="java.util.Date">
