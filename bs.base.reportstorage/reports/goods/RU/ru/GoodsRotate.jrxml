	<queryString><![CDATA[SELECT 

 P.PRODUCT_CODE,
 P.PRODUCT_NAME,
 U.NAME AS UNIT_NAME,
 SUM(R.REST_QUANTITY) AS REST_QUANTITY,
 SUM(R.REST_AMOUNT) AS REST_AMOUNT,

 SUM(R.CREDIT_QUANTITY) AS CREDIT_QUANTITY,
 SUM(R.CREDIT_AMOUNT) AS CREDIT_AMOUNT,

 SUM(R.DEBIT_QUANTITY) AS DEBIT_QUANTITY,
 SUM(R.DEBIT_AMOUNT) AS DEBIT_AMOUNT,

 SUM(R.REST_QUANTITY) + SUM(R.CREDIT_QUANTITY) - SUM(R.DEBIT_QUANTITY) AS END_QUANTITY,
 SUM(R.REST_AMOUNT) + SUM(R.CREDIT_AMOUNT) - SUM(R.DEBIT_AMOUNT) AS END_AMOUNT

 FROM V_PRODUCT P
 INNER JOIN UNIT U ON U.ID = P.UNIT_ID
 INNER JOIN V_RPT_GOODS_ROTATE R ON R.PRODUCT_ID = P.ID
 WHERE R.ORGANIZATION_ID = $P{ORGANIZATION_ID} AND R.PERIOD_ID  = $P{PERIOD_ID}
 GROUP BY P.PRODUCT_CODE, P.PRODUCT_NAME, U.NAME]]></queryString>

