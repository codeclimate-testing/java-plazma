

    <entity xmlns="http://plazmaforge.org/erm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="ContactType" class="org.plazmaforge.bs.contact.shared.entities.ContactType" table="CONTACT_TYPE">
