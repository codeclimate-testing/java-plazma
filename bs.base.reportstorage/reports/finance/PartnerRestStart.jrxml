	<queryString><![CDATA[SELECT 
  R.PERIOD_ID,
  P.ID AS PARTNER_ID,
  P.NAME AS PARTNER_NAME,
  SUM(R.DEBIT_AMOUNT) AS DEBIT_AMOUNT,
  SUM(R.CREDIT_AMOUNT) AS CREDIT_AMOUNT
 FROM PARTNER P
 LEFT JOIN V_RPT_PARTNER_REST_START R ON (P.ID = R.PARTNER_ID AND R.PERIOD_ID =  $P{PERIOD_ID} AND R.ORGANIZATION_ID =  $P{ORGANIZATION_ID})
 WHERE P.ID <> $P{ORGANIZATION_ID} AND (P.DESCRIMINATOR IS NULL OR P.DESCRIMINATOR <> 'BANK')
 GROUP BY  
  R.PERIOD_ID,
  P.ID,
  P.NAME
 ORDER BY P.NAME]]></queryString>

