<ui xmlns="http://plazmaforge.org/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form name="TaskResultListForm" type="ListForm" title="@{TaskResultListForm.title}">
		<content layout="FitLayout">
			<children>
				<table>
					<columns>
						<column text="@{TaskResultListForm.nameColumn.text}" property="name" data-type="String" width="200"/>
						<column text="@{TaskResultListForm.codeColumn.text}" property="code" data-type="String" width="100"/>
					</columns>
				</table>
			</children>
		</content>
	</form>
</ui>