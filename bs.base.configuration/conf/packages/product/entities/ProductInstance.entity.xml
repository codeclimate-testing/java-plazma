
    <entity xmlns="http://plazmaforge.org/erm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="ProductInstance" class="org.plazmaforge.bsolution.product.common.beans.ProductInstance" table="PRODUCT_INSTANCE">
