	<queryString><![CDATA[SELECT 
  R.PERIOD_ID,
  R.PRODUCT_ID,
  R.PRODUCT_CODE,
  R.PRODUCT_NAME,
  R.UNIT_ID,
  R.UNIT_NAME,
  SUM(R.QUANTITY) AS QUANTITY,
  SUM(R.AMOUNT / R.QUANTITY) AS PRICE,
  SUM(R.AMOUNT) AS AMOUNT,
  W.NAME AS WAREHOUSE_NAME,
  R.WAREHOUSE_ID
 FROM V_RPT_WRH_GOODS_REST_START R
 INNER JOIN V_WAREHOUSE W ON W.ID = R.WAREHOUSE_ID
 WHERE R.PERIOD_ID =  $P{PERIOD_ID}
  AND R.ORGANIZATION_ID =  $P{ORGANIZATION_ID}
 GROUP BY  
  R.PERIOD_ID,
  R.PRODUCT_ID,
  R.PRODUCT_CODE,
  R.PRODUCT_NAME,
  R.UNIT_ID,
  R.UNIT_NAME,
  W.NAME,
  R.WAREHOUSE_ID
