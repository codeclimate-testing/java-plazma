	<queryString><![CDATA[SELECT
 D.ID,
 D.DOCUMENT_NO,
 D.DOCUMENT_DATE,
 D.ORGANIZATION_ID,
 D.PARTNER_ID,

 O.NAME AS ORGANIZATION_NAME,
 O.NAME_EN AS ORGANIZATION_NAME_EN,
 O.NEC AS ORGANIZATION_NEC,
 C.ORGANIZATION_PERSON,
 C.ORGANIZATION_PERSON_EN,
 C.ORGANIZATION_PERSON_DETAIL,
 C.ORGANIZATION_PERSON_DETAIL_EN,
 C.ORGANIZATION_PERSON_ON_BAS,
 C.ORGANIZATION_PERSON_ON_BAS_EN,
 C.ORGANIZATION_ADDRESS,
 C.ORGANIZATION_ADDRESS_EN,
 C.ORGANIZATION_PHONE,
 C.ORGANIZATION_FAX,
 OA.ACCOUNT_NO AS ORGANIZATION_ACCOUNT_NO,
 OB.BIC AS ORGANIZATION_BANK_BIC,
 C.ORGANIZATION_BANK_NAME,
 C.ORGANIZATION_BANK_NAME_EN,

 P.NAME AS PARTNER_NAME,
 P.NAME_EN AS PARTNER_NAME_EN,
 P.NEC AS PARTNER_NEC,
 C.PARTNER_PERSON,
 C.PARTNER_PERSON_EN,
 C.PARTNER_PERSON_DETAIL,
 C.PARTNER_PERSON_DETAIL_EN,
 C.PARTNER_PERSON_ON_BAS,
 C.PARTNER_PERSON_ON_BAS_EN,
 C.PARTNER_ADDRESS,
 C.PARTNER_ADDRESS_EN,
 C.PARTNER_PHONE,
 C.PARTNER_FAX,
 PA.ACCOUNT_NO AS PARTNER_ACCOUNT_NO,
 PB.BIC AS PARTNER_BANK_BIC,
 C.PARTNER_BANK_NAME,
 C.PARTNER_BANK_NAME_EN


FROM 
 DOCUMENT D
 INNER JOIN FINANCE_DOCUMENT F ON D.ID = F.ID
 INNER JOIN CONTRACT C ON D.ID = C.ID
 INNER JOIN PARTNER O ON O.ID = D.ORGANIZATION_ID
 INNER JOIN PARTNER P ON P.ID = D.PARTNER_ID
 LEFT JOIN BANK_ACCOUNT OA ON OA.ID = C.ORGANIZATION_BANK_ACCOUNT_ID
 LEFT JOIN BANK_ACCOUNT PA ON PA.ID = C.PARTNER_BANK_ACCOUNT_ID
 LEFT JOIN V_BANK OB ON OB.ID = OA.BANK_ID
 LEFT JOIN V_BANK PB ON PB.ID = PA.BANK_ID
WHERE D.ID = $P{ID}]]></queryString>

