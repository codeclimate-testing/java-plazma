package org.plazmaforge.bsolution.payroll.common.beans;

import org.plazmaforge.framework.ext.model.BaseEntity;

/**
 * @author Oleh Hapon
 * Date: 24.10.2004
 * Time: 15:40:52
 * $Id: EmployeeVacationAverange.java,v 1.3 2010/12/05 07:55:59 ohapon Exp $
 */
public class EmployeeVacationAverange extends BaseEntity {

    private EmployeeVacation employeeVacation;


    private int month;
    private int workDay;
    private int vacationDay;
    private int addVacationDay;
    private double amount;


    public EmployeeVacation getEmployeeVacation() {
        return employeeVacation;
    }
    
    public void setEmployeeVacation(EmployeeVacation employeeVacation) {
        this.employeeVacation = employeeVacation;
    }
    

    public int getMonth() {
        return month;
    }

    public void setMonth(int month) {
        this.month = month;
    }

    public int getWorkDay() {
        return workDay;
    }

    public void setWorkDay(int workDay) {
        this.workDay = workDay;
    }

    public int getVacationDay() {
        return vacationDay;
    }

    public void setVacationDay(int vacationDay) {
        this.vacationDay = vacationDay;
    }

    public int getAddVacationDay() {
        return addVacationDay;
    }

    public void setAddVacationDay(int addVacationDay) {
        this.addVacationDay = addVacationDay;
    }

    public double getAmount() {
        return amount;
    }

    public void setAmount(double amount) {
        this.amount = amount;
    }
}
