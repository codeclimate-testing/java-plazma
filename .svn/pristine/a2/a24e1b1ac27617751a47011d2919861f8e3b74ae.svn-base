	<queryString><![CDATA[SELECT
 E.ID AS EMPLOYEE_ID,
 E.CODE AS EMPLOYEE_CODE,
 C.LAST_NAME,
 C.FIRST_NAME,
 C.MIDDLE_NAME,
 D.NAME AS DEPARTMENT_NAME,
 D.ID AS DEPARTMENT_ID,
 T.WORK_DAY,
 T.WORK_HOUR,
 T.PAYROLL_AMOUNT,
 T.START_BALANCE,
 R.PAYROLL_DAY,
 R.PAYROLL_HOUR,
 R.AMOUNT,
 Y.CODE AS PAYROLL_CODE,
 Y.NAME AS PAYROLL_NAME,
 Y.PAYROLL_TYPE_ID
FROM CONTACT C
INNER JOIN EMPLOYEE E ON E.PERSON_ID = C.ID
LEFT JOIN DEPARTMENT D ON E.DEPARTMENT_ID = D.ID
INNER JOIN EMPLOYEE_TOTAL T ON T.EMPLOYEE_ID = E.ID AND T.PERIOD_ID = $P{PERIOD_ID}
INNER JOIN EMPLOYEE_PAYROLL R ON R.EMPLOYEE_ID = E.ID AND R.PERIOD_ID = $P{PERIOD_ID}
INNER JOIN PAYROLL Y ON Y.ID = R.PAYROLL_ID
WHERE E.ORGANIZATION_ID = $P{ORGANIZATION_ID}
ORDER BY D.CODE, D.ID, E.CODE, E.ID, Y.PAYROLL_TYPE_ID]]></queryString>

