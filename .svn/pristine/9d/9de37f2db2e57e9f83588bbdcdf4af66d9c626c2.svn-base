<ui xmlns="http://plazmaforge.org/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form name="FormPropertyListForm" type="ListForm" title="@{FormPropertyListForm.title}">
		<content layout="FitLayout">
			<children>
				<table>
					<columns>
						<column text="@{FormPropertyListForm.nameColumn.text}" property="name" data-type="String" width="200"/>
						<column text="@{FormPropertyListForm.fullNameColumn.text}" property="fullName" data-type="String" width="300"/>
					</columns>
				</table>
			</children>
		</content>
	</form>
</ui>