	<queryString><![CDATA[SELECT

  D.PARTNER_ID,
  D.ORGANIZATION_ID,
  R.NAME AS ORGANIZATION_NAME,
  A.FULL_ADDRESS_STRING AS ADDRESS_STRING,
  H.PHONE_STRING,
  D.DOCUMENT_NO,
  D.DOCUMENT_DATE,
  I.PRODUCT_ID,
  I.PRODUCT_INSTANCE_ID,
  I.PRODUCT_FEATURE_NAME,
  P.PRODUCT_CODE,
  P.PRODUCT_NAME,
  E.SERIAL_NUMBER,
  E.NAME AS INSTANCE_NAME,
  P.WARRANTY_PERIOD

FROM
 DOCUMENT D
 INNER JOIN GOODS_OUTCOME G ON (D.ID = G.ID)
 INNER JOIN GOODS_OUTCOME_ITEM I ON (I.GOODS_OUTCOME_ID = G.ID)
 INNER JOIN PRODUCT_INSTANCE E ON (E.ID = I.PRODUCT_INSTANCE_ID)
 INNER JOIN V_PRODUCT P ON (P.ID = I.PRODUCT_ID)
 INNER JOIN PARTNER R ON (R.ID = D.ORGANIZATION_ID)
 LEFT JOIN V_DEF_PARTNER_ADDRESS A ON (A.PARTNER_ID = R.ID)
 LEFT JOIN V_DEF_PARTNER_PHONE H ON (H.PARTNER_ID = R.ID)
 WHERE D.ID = $P{ID}]]></queryString>

