<ui xmlns="http://plazmaforge.org/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form name="ProjectRoleListForm" type="ListForm" title="@{ProjectRoleListForm.title}">
		<content layout="FitLayout">
			<children>
				<table>
					<columns>
						<column text="@{ProjectRoleListForm.nameColumn.text}" property="name" data-type="String" width="200"/>
						<column text="@{ProjectRoleListForm.descriptionColumn.text}" property="description" data-type="String" width="200"/>
						<column text="@{ProjectRoleListForm.codeColumn.text}" property="code" data-type="String" width="100"/>
					</columns>
				</table>
			</children>
		</content>
	</form>
</ui>