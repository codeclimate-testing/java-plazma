<ui xmlns="http://plazmaforge.org/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form name="AddressTypeListForm" type="ListForm" title="@{AddressTypeListForm.title}">
		<content layout="FitLayout">
			<children>
				<table>
					<columns>
						<column text="@{AddressTypeListForm.codeColumn.text}" property="code" data-type="String" width="200"/>
						<column text="@{AddressTypeListForm.nameColumn.text}" property="name" data-type="String" width="200"/>
					</columns>
				</table>
			</children>
		</content>
	</form>
</ui>