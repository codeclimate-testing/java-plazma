	<queryString><![CDATA[SELECT
 E.CODE,
 C.LAST_NAME,
 C.FIRST_NAME,
 C.MIDDLE_NAME,
 D.NAME AS DEPARTMENT_NAME,
 D.ID AS DEPARTMENT_ID,
 T.PAYROLL_AMOUNT
FROM CONTACT C
INNER JOIN EMPLOYEE E ON E.PERSON_ID = C.ID
INNER JOIN EMPLOYEE_TOTAL T ON E.ID = T.EMPLOYEE_ID
LEFT JOIN DEPARTMENT D ON E.DEPARTMENT_ID = D.ID
WHERE E.ORGANIZATION_ID = $P{ORGANIZATION_ID} AND T.PERIOD_ID = $P{PERIOD_ID}
ORDER BY D.CODE, D.ID]]></queryString>

