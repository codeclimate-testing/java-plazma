package org.plazmaforge.bsolution.finance.common.beans;

import org.plazmaforge.bsolution.bank.common.beans.BankAccount;
import org.plazmaforge.bsolution.contact.common.beans.ContactTitle;


/** 
 * @author Oleh Hapon
 * $Id: ContractDetails.java,v 1.2 2010/04/28 06:24:25 ohapon Exp $
 */

public class ContractDetails {

    private ContactTitle contact;
    
    private BankAccount bankAccount;
    
    
    private String person;
    
    private String personEn;
    
    
    private String personOnBasis;
    
    private String personOnBasisEn;
    
    private String personDetail;
    
    private String personDetailEn;

    ////
    
    private String address;
    
    private String addressEn;
    
    private String phone;
    
    private String fax;
    
    ////
    
    
    private String bankName;
    
    private String bankNameEn;
    
    
    public ContactTitle getContact() {
        return contact;
    }

    public void setContact(ContactTitle contact) {
        this.contact = contact;
    }
    
    public BankAccount getBankAccount() {
        return bankAccount;
    }

    public void setBankAccount(BankAccount bankAccount) {
        this.bankAccount = bankAccount;
    }


    ///
    
    public String getPerson() {
        return person;
    }

    public void setPerson(String person) {
        this.person = person;
    }

    public String getPersonDetail() {
        return personDetail;
    }

    public void setPersonDetail(String personDetail) {
        this.personDetail = personDetail;
    }

    public String getPersonDetailEn() {
        return personDetailEn;
    }

    public void setPersonDetailEn(String personDetailEn) {
        this.personDetailEn = personDetailEn;
    }

    public String getPersonEn() {
        return personEn;
    }

    public void setPersonEn(String personEn) {
        this.personEn = personEn;
    }

    public String getPersonOnBasis() {
        return personOnBasis;
    }

    public void setPersonOnBasis(String personOnBasis) {
        this.personOnBasis = personOnBasis;
    }

    public String getPersonOnBasisEn() {
        return personOnBasisEn;
    }

    public void setPersonOnBasisEn(String personOnBasisEn) {
        this.personOnBasisEn = personOnBasisEn;
    }
    
    ////
    
    
    public String getAddress() {
        return address;
    }

    public void setAddress(String address) {
        this.address = address;
    }

    public String getAddressEn() {
        return addressEn;
    }

    public void setAddressEn(String addressEn) {
        this.addressEn = addressEn;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }
    
    public String getFax() {
        return fax;
    }

    public void setFax(String fax) {
        this.fax = fax;
    }
    
    ////

    public String getBankName() {
        return bankName;
    }

    public void setBankName(String bankName) {
        this.bankName = bankName;
    }

    public String getBankNameEn() {
        return bankNameEn;
    }

    public void setBankNameEn(String bankNameEn) {
        this.bankNameEn = bankNameEn;
    }

    
}
