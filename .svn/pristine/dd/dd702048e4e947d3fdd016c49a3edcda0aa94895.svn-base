
    <entity xmlns="http://plazmaforge.org/erm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="ProjectStage" class="org.plazmaforge.bsolution.project.common.beans.ProjectStage" table="PROJECT_STAGE">
