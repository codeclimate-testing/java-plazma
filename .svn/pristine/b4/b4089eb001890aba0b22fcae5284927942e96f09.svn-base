<ui xmlns="http://plazmaforge.org/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form name="LanguageEditForm" type="EditForm" title="@{LanguageEditForm.title}">
		<content layout="GridLayout(columnCount=2)">
			<children>
				<label text="@{LanguageEditForm.codeLabel.text}"/><text-field property="code" width="100"/>
				<label text="@{LanguageEditForm.code3Label.text}"/><text-field property="code3" width="100"/>
				<label text="@{LanguageEditForm.numericCodeLabel.text}"/><text-field property="numericCode" width="100"/>
				<label text="@{LanguageEditForm.nameLabel.text}"/><text-field property="name" width="200"/>
				
			</children>
		</content>
	</form>
</ui>