		<content layout="FitLayout">
			<children>
				<table>
					<columns>
						<column text="@{ContactTypeListForm.codeColumn.text}" property="code" data-type="String" width="200"/>
						<column text="@{ContactTypeListForm.nameColumn.text}" property="name" data-type="String" width="200"/>
					</columns>
				</table>
			</children>
		</content>
	</form>
</ui>