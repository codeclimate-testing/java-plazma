	<queryString><![CDATA[SELECT 
 FD.ORGANIZATION_NAME,
 FD.DOCUMENT_NO,
 FD.DOCUMENT_DATE,
 W.NAME AS WAREHOUSE_NAME,
 T.NAME AS PARTNER_NAME,
 FD.CURRENCY_NAME,
 FD.CURRENCY_RATE,
 FD.CURRENCY_RATE_DATE,
 P.PRODUCT_NAME,
 I.PRODUCT_FEATURE_NAME,
 U.NAME AS UNIT_NAME,
 I.QUANTITY,
 I.CURRENCY_PRICE_WITH_TAX AS CURRENCY_ITEM_PRICE,
 I.CURRENCY_AMOUNT_WITH_TAX AS CURRENCY_ITEM_AMOUNT,
 FD.CURRENCY_DOCUMENT_AMOUNT
 FROM PURCHASE_ORDER D
 INNER JOIN V_FINANCE_DOCUMENT FD ON FD.ID = D.ID
 INNER JOIN PARTNER T ON T.ID = FD.PARTNER_ID
 LEFT JOIN V_WAREHOUSE W ON W.ID = D.WAREHOUSE_ID
 INNER JOIN PURCHASE_ORDER_ITEM I ON I.PURCHASE_ORDER_ID = D.ID
 INNER JOIN V_PRODUCT P ON P.ID = I.PRODUCT_ID
 INNER JOIN UNIT U ON U.ID = I.UNIT_ID

 WHERE D.ID = $P{ID}]]></queryString>

