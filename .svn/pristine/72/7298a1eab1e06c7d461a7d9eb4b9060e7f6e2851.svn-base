<ui xmlns="http://plazmaforge.org/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form name="TaskPriorityListForm" type="ListForm" title="@{TaskPriorityListForm.title}">
		<content layout="FitLayout">
			<children>
				<table>
					<columns>
						<column text="@{TaskPriorityListForm.nameColumn.text}" property="name" data-type="String" width="200"/>
						<column text="@{TaskPriorityListForm.codeColumn.text}" property="code" data-type="String" width="100"/>
						<column text="@{TaskPriorityListForm.orderNumberColumn.text}" property="orderNumber" data-type="Integer" width="100"/>
					</columns>
				</table>
			</children>
		</content>
	</form>
</ui>