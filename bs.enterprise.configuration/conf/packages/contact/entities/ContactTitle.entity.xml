

    <entity xmlns="http://plazmaforge.org/erm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="ContactTitle" class="org.plazmaforge.bs.contact.shared.entities.ContactTitle" table="CONTACT" readonly="true">
